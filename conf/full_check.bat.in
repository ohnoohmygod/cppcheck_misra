@echo off

REM 检查当前路径下是否存在 full-check-config.yaml
if not exist "full-check-config.yaml" (
    echo can not find full-check-config.yaml, please run the following command：
    echo misra --load_config
    pause
    exit /b 1
)

REM 如果文件存在，继续执行后续命令
echo find full-check-config.yaml, start incremental check...
misra --full

REM 通过任意键退出
pause
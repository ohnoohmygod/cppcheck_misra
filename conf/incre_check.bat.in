@echo off

REM check  increment-check-config.yaml is exsit
if not exist "increment-check-config.yaml" (
    echo can not find increment-check-config.yaml ，please run following command：
    echo misra --load_config
    pause
    exit /b 1
)

REM If the increment-check-config.yaml file exists, start the incremental check.
echo find increment-check-config.yaml is exist，start incremental check...
misra --incre

REM input any key to exit
pause
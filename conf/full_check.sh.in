#!/bin/bash

# 检查当前路径下是否存在 full-check-config.yaml
if [ ! -f "full-check-config.yaml" ]; then
    echo "can not find full-check-config.yaml ，please run following command："
    echo "misra --load_config"
    exit 1  # 退出脚本，返回错误码 1
fi

# 如果文件存在，继续执行后续命令
echo "find full-check-config.yaml is exsit，start full check..."
misra --full
#!/bin/bash

# 检查当前路径下是否存在 increment-check-config.yaml
if [ ! -f "increment-check-config.yaml" ]; then
    echo "can not find increment-check-config.yaml, please check it"
    echo "misra --load_config"
    exit 1  # 退出脚本，返回错误码 1
fi

# 如果文件存在，继续执行后续命令
echo "find increment-check-config.yaml, start full check..."
misra --incre
